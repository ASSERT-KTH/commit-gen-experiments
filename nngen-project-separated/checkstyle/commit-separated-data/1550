 - { 
 - 	 / / nonPrivate ctor 
 - 	 public anotherNonFinalClass ( ) { } 	 
 - public void someMethod ( ) 
 - { 
 - 	 System . out . println ( " nonempty and overriding is possible " ) ; 
 - } 
 - } 
