 + @ interface Annotation 
 + { 
 + public String s1 = " " ; / / violation 
 + final String s2 = " " ; / / violation 
 + static String s3 = " " ; / / violation 
 + String s4 = " " ; 
 + public String blah ( ) ; / / violation 
 + abstract String blah2 ( ) ; / / violation 
 + } 
